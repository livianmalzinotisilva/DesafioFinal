{
    "_version": "1.32.0",
    "sap.app": {
        "id": "app.l4desafio.app",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach"
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "app.l4desafio.app.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        },
        "dependencies": {
            "minUI5Version": "1.66.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "app.l4desafio.app.i18n.i18n"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "app.l4desafio.app.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteApp",
                    "pattern": "RouteApp",
                    "target": ["TargetApp"]
                },
                {
				"name": "Menu",
				"pattern": "",
				"target": ["Menu","Relatorio"]
                },
                {
                "name": "Relatorio",
                "pattern": "Relatorio",
                "target": ["Menu", "Relatorio"]
                },
                {
                "name": "Gerenciar",
                "pattern": "Gerenciar",
                "target": ["Menu", "Gerenciar"]
                }
            ],
            "targets": {
                 "Menu":{
                   "viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "menu",
                    "viewName": "Menu",
                    "controlAggregation": "masterPages"           
                },
                "TargetApp": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "App",
                    "viewName": "App"
                },
                "Relatorio": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "relatorio",
                    "viewName": "Relatorio",
                    "controlAggregation": "detailPages"
                },
                "Gerenciar": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "gerenciar",
                    "viewName": "Gerenciar",
                    "controlAggregation": "detailPages"
                }
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "app-l4desafio-app"
    }
}